{
  "title": "Exercise 3-10",
  "description": "Here is a list of data to practice with. Every time you re-visit or re-load the page, the data will be different.",
  "showQuestionNumbers": "off",
  "showProgressBar": "off",
  "showNavigationButtons": false,
  "showCompletedPage": false,
  "checkErrorsMode": "onValueChanged",
  "showCorrectAnswer": true,
  "calculatedValues": [
    {
      "name": "randomData",
      "expression": "generateRandomData(25, -50, 50)"
    },
    {
      "name": "sortedData", 
      "expression": "sortData({randomData})"
    },
    {
      "name": "percentiles",
      "expression": "generateRandomPercentiles(5, 1, 99)"
    },
    {
      "name": "percentileAnswers",
      "expression": "calculatePercentiles({sortedData}, {percentiles})"
    },
    {
      "name": "ordinalPercentile1",
      "expression": "getOrdinal({percentiles}[0])"
    },
    {
      "name": "ordinalPercentile2",
      "expression": "getOrdinal({percentiles}[1])"
    },
    {
      "name": "ordinalPercentile3",
      "expression": "getOrdinal({percentiles}[2])"
    },
    {
      "name": "ordinalPercentile4",
      "expression": "getOrdinal({percentiles}[3])"
    },
    {
      "name": "ordinalPercentile5",
      "expression": "getOrdinal({percentiles}[4])"
    }
  ],
  "questions": [
    {
      "type": "text",
      "name": "p1",
      "title": "Practice Data: {randomData}\n\nSorted Data: {sortedData}\n\n{ordinalPercentile1} percentile:",
      "correctAnswer": "{percentileAnswers}.p1"
    },
    {
      "type": "text", 
      "name": "p2",
      "title": "{ordinalPercentile2} percentile:",
      "correctAnswer": "{percentileAnswers}.p2"
    },
    {
      "type": "text",
      "name": "p3", 
      "title": "{ordinalPercentile3} percentile:",
      "correctAnswer": "{percentileAnswers}.p3"
    },
    {
      "type": "text",
      "name": "p4",
      "title": "{ordinalPercentile4} percentile:",
      "correctAnswer": "{percentileAnswers}.p4"
    },
    {
      "type": "text",
      "name": "p5",
      "title": "{ordinalPercentile5} percentile:",
      "correctAnswer": "{percentileAnswers}.p5"
    }
  ]
}
